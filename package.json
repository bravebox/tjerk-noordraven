{
  "name": "tjerknoordraven-prismic",
  "private": true,
  "description": "De Tjerk Noordraven website",
  "version": "1.0.2",
  "license": "MIT",
  "scripts": {
    "deploy": "rm -Rf .cache && gatsby build --prefix-paths && gh-pages -d public",
    "build": "rm -Rf .cache && gatsby build",
    "develop": "rm -Rf .cache && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "rm -Rf .cache && npm run develop",
    "serve": "rm -Rf .cache && gatsby serve"
  },
  "dependencies": {
    "gatsby": "^2.20.22",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-google-analytics": "^2.1.23",
    "gatsby-plugin-react-helmet": "^3.1.8",
    "gatsby-plugin-sass": "^2.1.15",
    "gatsby-plugin-sitemap": "^2.2.17",
    "gatsby-source-prismic": "^2.2.0",
    "gatsby-source-prismic-graphql": "^3.6.2",
    "node-sass": "^4.12.0",
    "prismic.io": "^3.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-custom-share": "^0.4.8",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "dotenv": "^8.1.0",
    "prettier": "^1.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bravebox/tjerknoordraven-prismic/"
  }
}
